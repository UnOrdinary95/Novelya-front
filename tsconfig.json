{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve",
    "paths": {
      "@spartan-ng/helm/button": [
        "./src/app/shared/libs/ui/button/src/index.ts"
      ],
      "@spartan-ng/helm/utils": [
        "./src/app/shared/libs/ui/utils/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        "./src/app/shared/libs/ui/icon/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        "./src/app/shared/libs/ui/input/src/index.ts"
      ],
      "@spartan-ng/helm/form-field": [
        "./src/app/shared/libs/ui/form-field/src/index.ts"
      ],
      "@spartan-ng/helm/field": [
        "./src/app/shared/libs/ui/field/src/index.ts"
      ],
      "@spartan-ng/helm/label": [
        "./src/app/shared/libs/ui/label/src/index.ts"
      ],
      "@spartan-ng/helm/separator": [
        "./src/app/shared/libs/ui/separator/src/index.ts"
      ],
      "@spartan-ng/helm/checkbox": [
        "./src/app/shared/libs/ui/checkbox/src/index.ts"
      ],
      "@spartan-ng/helm/badge": [
        "./src/app/shared/libs/ui/badge/src/index.ts"
      ],
      "@spartan-ng/helm/card": [
        "./src/app/shared/libs/ui/card/src/index.ts"
      ],
      "@/*": [
        "./src/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@core/*": [
        "./src/app/core/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@features/*": [
        "./src/app/features/*"
      ],
      "@environments/*": [
        "./src/environments/*"
      ],
      "@spartan-ng/helm/sonner": [
        "./src/app/shared/libs/ui/sonner/src/index.ts"
      ],
      "@spartan-ng/helm/tabs": [
        "./src/app/shared/libs/ui/tabs/src/index.ts"
      ],
      "@spartan-ng/helm/dialog": [
        "./src/app/shared/libs/ui/dialog/src/index.ts"
      ],
      "@spartan-ng/helm/alert-dialog": [
        "./src/app/shared/libs/ui/alert-dialog/src/index.ts"
      ],
      "@spartan-ng/helm/avatar": [
        "./src/app/shared/libs/ui/avatar/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
